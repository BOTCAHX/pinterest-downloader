{
  "version": 2,
  "name": "pinterest-downloader",
  "builds": [
    {
      "src": "package.json", 
      "use": "@vercel/static-build",
      "config": { 
        "distDir": "dist",
        "framework": "vue"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "github": {
    "silent": true
  },
  "env": {
    "NODE_ENV": "production"
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}
